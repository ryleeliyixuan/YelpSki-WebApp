<% feed.forEach(function(feedItem) { %>
<div class="card mb-3">
  <div class="row">
    <div class="col-md-4">
      <img src="<%= feedItem.imageSource %>" alt="Placeholder image" />
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title"><%= feedItem.name %></h5>

        <p class="card-text"><%= feedItem.message %></p>
        <p class="card-text">
          <small class="text-muted"><%= feedItem.nameHandle%></small>
        </p>
        <a class="btn btn-primary">View <%=feedItem.title%></a>
      </div>
    </div>
  </div>
</div>
<% }) %>
